# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
RAW_FRAME_INPUT_CHANNEL=camera:frames:rpi
ANNOTATED_FRAME_OUTPUT_CHANNEL=ai_stream:frames:rpi

# AI Model Configuration - CHANGED TO YOLOV8S
AI_MODEL_PATH=yolov8s.hef

# Performance Optimization Parameters
PERFORMANCE_MODE=REALTIME
MAX_QUEUE_SIZE=1
FRAME_DROP_THRESHOLD=2
PROCESSING_THREADS=1
INFERENCE_TIMEOUT=0.05

# Image Processing Parameters - Optimized for YOLOv8s
MODEL_INPUT_WIDTH=640
MODEL_INPUT_HEIGHT=640
RESIZE_INTERPOLATION=NEAREST
JPEG_QUALITY=90
CONFIDENCE_THRESHOLD=0.3

# Latency Optimization - Enhanced for YOLOv8s speed
REDIS_DECODE_RESPONSES=false
PUBSUB_TIMEOUT=0.01
SKIP_FRAME_RATIO=2
ENABLE_FRAME_DROPPING=true
CLEAR_QUEUE_ON_START=true

# Debug and Monitoring
ENABLE_VERBOSE_OVERLAY=true
OVERLAY_POSITION=TOP_LEFT
OVERLAY_FONT_SIZE=0.6
OVERLAY_BACKGROUND=true
SHOW_FRAME_DETAILS=true
SHOW_MODEL_DETAILS=true
SHOW_PROCESSING_METRICS=true
SHOW_DETECTION_DETAILS=true
SHOW_SYSTEM_METRICS=true
MAX_DETECTION_LINES=8
OVERLAY_UPDATE_INTERVAL=1